                        <xsl:value-of select="."/>
                    </xsl:element>
                    <xsl:text> (</xsl:text>
                    <xsl:value-of select="ancestor::place/desc"/>
                    <xsl:text>)</xsl:text>
                    <xsl:variable name="Placeid">
                        <xsl:value-of select="ancestor::place/@xml:id"/>
                    </xsl:variable>
                    <xsl:text> : </xsl:text>
                    <xsl:for-each select="ancestor::TEI//text//placeName[translate(@ref, '#','')=$Placeid]">
